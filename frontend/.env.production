# Production Environment Variables for Frontend
REACT_APP_API_URL=https://api.study-ai.io
REACT_APP_DOMAIN=study-ai.io
REACT_APP_FRONTEND_URL=https://study-ai.io
REACT_APP_ENV=production

# Development fallbacks
REACT_APP_DEV_API_URL=http://localhost:3001
REACT_APP_DEV_FRONTEND_URL=http://localhost:3000

# Feature flags
REACT_APP_GOOGLE_DRIVE_ENABLED=true
REACT_APP_ONEDRIVE_ENABLED=true
REACT_APP_ICLOUD_ENABLED=true
REACT_APP_SUPABASE_ENABLED=true

# Analytics
REACT_APP_ENABLE_ANALYTICS=true